{
  "name": "paragonie/stern",
  "description": "Stern lets you built type-safe PHP projects, even if your project's users aren't writing type-safe code",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ParagonIE\\Stern\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParagonIE\\SternTests\\": "tests/test-classes"
    }
  },
  "require": {
    "php": "^7|^8"
  },
  "require-dev": {
    "phpunit/phpunit": "^6|^7|^8|^9",
    "vimeo/psalm": "^1|^3|^4"
  }
}